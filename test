SELECT \`class_member\`.\`memberId\`,\`class_member\`.\`memberMemo\`,\`my_member\`.\`fullName\`,\`my_member\`.\`gender\`,\`my_member\`.\`email\`,\`my_member\`.\`mobileNumber\`
FROM \`class_member\` 
INNER JOIN \`my_member\`
ON \`class_member\`.\`memberId\` = \`my_member\`.\`memberId\`
WHERE \`class_member\`.\`classId\` = 'C20030001' 